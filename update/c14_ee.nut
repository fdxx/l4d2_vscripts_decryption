local eeButton = Entities.FindByName( null, "ee_button" );
if ( eeButton )
{
	eeButton.ConnectOutput( "OnPressed", "Pressed" );
	eeButton.ValidateScriptScope();
	local buttonScope = eeButton.GetScriptScope();
	buttonScope["UsedButton"] <- {};
	buttonScope.InputUse <- function()
	{
		if ( activator )
		{
			if ( GetCharacterDisplayName( activator ) in buttonScope.UsedButton )
				return false;
		}
		return true;
	}
	buttonScope.Pressed <- function()
	{
		if ( activator )
		{
			activator.GiveItemWithSkin( "crowbar", 1 );
			buttonScope.UsedButton[GetCharacterDisplayName( activator )] <- true;
		}
	}
}
